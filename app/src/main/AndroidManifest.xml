<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rahimeen.soufin.pullups">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.rahimeen.soufin.pullups.ParseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.rahimeen.soufin.pullups.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rahimeen.soufin.pullups.SingleItemView" >
        </activity>
        <activity android:name="com.rahimeen.soufin.pullups.ParseManager" >
        </activity>
        <activity
            android:name="com.rahimeen.soufin.pullups.Workout"
            android:label="@string/title_activity_workout" >
        </activity>
        <activity
            android:name="com.rahimeen.soufin.pullups.EndWorkout"
            android:label="@string/title_activity_end_workout" >
        </activity>

    </application>

</manifest>
